{
  "name": "jsbattle-webpage",
  "version": "2.7.1",
  "description": "",
  "main": "index.js",
  "watch": {
    "build:bundle": {
      "patterns": [
        "src/**/*.*"
      ],
      "verbose": true
    },
    "build:copy": {
      "patterns": [
        "static/**/*.*"
      ],
      "verbose": true
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src/",
    "build": "npm run build:copy && npm run build:bundle",
    "build:copy": "build/copy-libs.js",
    "build:bundle": "webpack --config ./webpack.config.js --mode=production",
    "test": "npm run test:dom && npm run test:browser",
    "test:dom": "jest -b=3",
    "test:browser": "cucumber-js -f ./features/support/SimpleFormatter.js --retry 3 --fail-fast --world-parameters '{\"mockserver\": true}'",
    "test:browser-smoke": "cucumber-js -t @smoke -f ./features/support/SimpleFormatter.js --retry 3 --fail-fast --world-parameters '{\"mockserver\": true}'",
    "analyse": "webpack --profile --json --config ./webpack.config.js > bundle_stats.json && webpack-bundle-analyzer bundle_stats.json",
    "watch": "npm-watch",
    "serve": "jsbattle-mock --port 9000",
    "start:dev": "webpack-dev-server --config ./webpack.config.js --mode=development"
  },
  "keywords": [],
  "author": "Krzysztof Jamroz <jamro@jamro.net>",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.4.1",
    "codemirror": "^5.52.2",
    "highlight.js": "^9.18.1",
    "jquery": "^3.5.0",
    "jsbattle-docs": "^2.7.1",
    "jsbattle-engine": "^2.7.0",
    "jsbattle-react": "^2.7.1",
    "pixi.js": "^5.2.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight": "^0.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "cpx": "^1.5.0",
    "cucumber": "^6.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-vue": "^6.2.2",
    "http-server": "^0.12.1",
    "jest": "^25.3.0",
    "jsbattle-mockserver": "^2.7.1",
    "json-server": "^0.16.1",
    "npm-watch": "^0.6.0",
    "pm2": "^4.2.3",
    "puppeteer": "^3.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "gitHead": "b086d7f5befb0506cc4f1ef9a6450cf2591b9b7d"
}
