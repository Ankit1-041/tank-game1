{
  "name": "jsbattle-webpage",
  "version": "3.3.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "build:bundle": {
      "patterns": [
        "src/**/*.*"
      ],
      "verbose": true
    },
    "build:copy": {
      "patterns": [
        "static/**/*.*"
      ],
      "verbose": true
    }
  },
  "scripts": {
    "start": "jsbattle-mock --port 9000",
    "start:dev": "webpack-dev-server --config ./webpack.config.js --mode=development",
    "clean": "rimraf dist",
    "lint": "eslint src/",
    "build": "npm run build:copy && npm run build:bundle",
    "build:copy": "build/copy-libs.js",
    "build:bundle": "webpack --config ./webpack.config.js --mode=production",
    "test": "npm run test:dom && npm run test:browser",
    "test:dom": "jest -b=3",
    "test:browser": "cucumber-js --tags \"not @integration_only\" -f ./features/support/SimpleFormatter.js --retry 3 --fail-fast --world-parameters '{\"mockserver\": true}'",
    "test:browser-smoke": "cucumber-js -t @smoke -f ./features/support/SimpleFormatter.js --retry 3 --fail-fast --world-parameters '{\"mockserver\": true}'",
    "analyse": "webpack --profile --json --config ./webpack.config.js > bundle_stats.json && webpack-bundle-analyzer bundle_stats.json",
    "watch": "npm-watch"
  },
  "keywords": [],
  "author": "Krzysztof Jamroz <jamro@jamro.net>",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.54.0",
    "highlight.js": "^10.0.3",
    "jquery": "~3.5.0",
    "jsbattle-docs": "^3.2.0",
    "jsbattle-engine": "^3.1.0",
    "jsbattle-react": "^3.1.0",
    "pixi.js": "^5.2.4",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight": "^0.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "cpx": "^1.5.0",
    "cucumber": "^6.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-vue": "^6.2.2",
    "http-server": "^0.12.3",
    "jest": "^26.0.1",
    "jsbattle-mockserver": "^3.3.0",
    "json-server": "^0.16.1",
    "npm-watch": "^0.6.0",
    "pm2": "^4.4.0",
    "puppeteer": "^3.3.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "gitHead": "b086d7f5befb0506cc4f1ef9a6450cf2591b9b7d"
}
