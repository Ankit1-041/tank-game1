{
  "name": "jsbattle",
  "version": "2.1.8",
  "description": "",
  "main": "dist/js/jsbattle.min.js",
  "scripts": {
    "start": "./node_modules/pm2/bin/pm2 start ./dist/ecosystem.config.js && ./node_modules/pm2/bin/pm2 logs --nostream --out --lines 5 jsbattle",
    "stop": "./node_modules/pm2/bin/pm2 stop ./dist/ecosystem.config.js | ./node_modules/pm2/bin/pm2 del ./dist/ecosystem.config.js",
    "restart": "./node_modules/pm2/bin/pm2 restart ./dist/ecosystem.config.js",
    "clean": "rimraf dist",
    "build": "npm run build:server && npm run build:webpage",
    "build:webpage": "cpx './node_modules/jsbattle-webpage/dist/**/*' ./dist/public",
    "build:server": "cpx './src/**/*' ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamro/jsbattle"
  },
  "author": "Krzysztof Jamroz <jamro@jamro.net>",
  "license": "MIT",
  "bin": {
    "jsbattle": "./dist/jsbattle.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "jsbattle-docs": "^2.1.8",
    "jsbattle-engine": "^2.1.8",
    "jsbattle-server": "^2.1.8",
    "jsbattle-webpage": "^2.1.8",
    "path": "^0.12.7",
    "pm2": "^3.2.9",
    "yargs": "^12.0.5"
  },
  "gitHead": "b086d7f5befb0506cc4f1ef9a6450cf2591b9b7d"
}
